{
  "name": "defi-heatmap-nuxt-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pgrep -f yarn | xargs kill -9 && nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "eslint --ext .ts,.js,.vue .",
    "test": "jest",
    "generate-types": "graphql-codegen --require dotenv/config  --config codegen/codegen.yml",
    "generate-erc20-abi": "abi-types-generator './constracts/abi/erc20Abi.json' --output='./types/abi' --name=erc20-contract --provider=ethers_v5",
    "generate-aave-lending-pool-abi": "abi-types-generator './constracts/abi/aave/lendingPoolAbi.json' --output='./types/abi' --name=aave-lending-pool-contract --provider=ethers_v5",
    "generate-aave-weth-pool-abi": "abi-types-generator './constracts/abi/aave/wethGatewayAbi.json' --output='./types/abi' --name=aave-weth-pool-contract --provider=ethers_v5",
    "generate-uniswap-router-v2": "abi-types-generator './constracts/abi/uniswapRouterV2Abi.json' --output='./types/abi' --name=uniswap-router-v2-contract --provider=ethers_v5"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.25",
    "@nuxtjs/apollo": "^4.0.1-rc.5",
    "@nuxtjs/composition-api": "^0.33.1",
    "@uniswap/sdk": "^3.0.3",
    "@uniswap/smart-order-router": "^ 2.8.1",
    "@vue/apollo-composable": "^4.0.0-alpha.19",
    "bignumber.js": "^9.1.1",
    "blockies-ts": "^1.0.0",
    "class-transformer": "^0.3.1",
    "cookie-universal-nuxt": "^2.2.1",
    "core-js": "^3.25.3",
    "ethers": "^5.6.9",
    "graphql-tag": "^2.12.5",
    "mitt": "^3.0.0",
    "nuxt": "^2.15.8",
    "transformer": "^1.2.8",
    "vue": "^2.7.10",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10",
    "vue-typer": "^1.2.0",
    "vuetify": "^2.6.10",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@graphql-codegen/cli": "^2.6.3",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-graphql-files-modules": "^2.1.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@nuxt/test-utils": "^3.6.1",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@types/jest": "^29.5.2",
    "@types/web3": "^1.2.2",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "ethereum-abi-types-generator": "^1.3.2",
    "jest": "^29.5.0",
    "nuxt-compress": "^5.0.0",
    "prettier": "^2.7.1",
    "ts-jest": "^27.1.5",
    "vue-jest": "^4.0.1",
    "postcss": "^8.1.9",
    "postcss-import": "12.0.1"
  }
}
